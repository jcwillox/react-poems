{
  "name": "individual-project-jcwillox",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "node build-api/api/server.js",
    "start:api": "ts-node -P tsconfig.api.json src/api/server.ts",
    "watch": "concurrently 'npm:watch:api' 'npm:watch:web' -c green.bold,magenta.bold",
    "watch:api": "ts-node-dev --rs --respawn -P tsconfig.api.json src/api/server.ts",
    "watch:web": "react-scripts start",
    "build": "concurrently 'npm:build:api' 'npm:build:web' -c green.bold,magenta.bold",
    "build:api": "tsc -p tsconfig.api.json",
    "build:web": "react-scripts build",
    "clean": "del-cli build build-api",
    "test": "react-scripts test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier src/ --write",
    "format:check": "prettier src/ --check",
    "generate:poem": "ts-node -P tsconfig.api.json src/scripts/generate-poem.ts",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "clipboard-copy": "^4.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jdenticon": "^3.1.0",
    "markdown-to-jsx": "^7.1.3",
    "morgan": "^1.10.0",
    "notistack": "^1.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "use-debounce": "^7.0.0",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.3.0",
    "del-cli": "^4.0.1",
    "prettier": "^2.4.1",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "16.x",
    "npm": ">=7.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
